---
"$extends":
  - base/normal.json
when:
  source:
    - ${BUD_LIB}/strings.rc
  cmd: urldecode
  args:
    - %E3%81%93%E3%82%93%E3%81%AB%E3%81%A1%E3%81%AF%E3%80%81%E4%B8%96%E7%95%8C
then:
  exitCode:
    - EQUAL
    - 0
  stdout:
    present:
      - こんにちは、世界
  stderr:
    absent:
      - REGEX:.+
